{{heading-depth-bump~}}
{{#if (findWhere @root longname=longname kind="class")~}}
  {{#with (findWhere @root longname=longname kind="class")~}}
    {{>class~}}
  {{/with~}}
{{else~}}
  {{#if (findWhere @root longname=longname kind="function")~}}
    {{#with (findWhere @root longname=longname kind="function")~}}
      {{>function~}}
    {{/with~}}
  {{else~}}
    {{>module-index~}}
    {{>child-docs~}}
  {{/if~}}
{{/if~}}
{{heading-depth-drop~}}