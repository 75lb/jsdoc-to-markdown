{
  "name": "jsdoc-to-markdown",
  "author": "Lloyd Brookes",
  "version": "0.2.1",
  "description": "Render JSdoc documentation as markdown",
  "repository": "https://github.com/75lb/jsdoc-to-markdown",
  "main": "lib/jsdoc-to-markdown.js",
  "scripts": {
    "test": "",
    "docs": "node test/render.js",
    "watch": "baldrick --do 'npm run docs' --when test/input/*/*.js partials/* --change --speak",
    "lint": "jshint helpers/*.js bin/*.js lib/*.js; echo"
  },
  "bin": {
    "jsdoc2md": "bin/cli.js"
  },
  "keywords": [
    "handlebars",
    "jsdoc",
    "markdown",
    "generator"
  ],
  "dependencies": {
    "array-tools": "^1.1.0",
    "boil-js": "^0.1.0",
    "command-line-args": "^0.3.0",
    "console-dope": "^0.3.3",
    "file-set": "^0.2",
    "jsdoc": "^3.3.0-alpha5",
    "more-fs": "^0.4.0",
    "object-tools": "^1.0.0"
  }
}
