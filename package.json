{
  "name": "jsdoc-to-markdown",
  "author": "Lloyd Brookes",
  "version": "0.6.4",
  "description": "Markdown API documentation generator",
  "repository": "https://github.com/75lb/jsdoc-to-markdown",
  "main": "lib/jsdoc-to-markdown.js",
  "files": [
    "bin",
    "lib",
    "LICENSE"
  ],
  "scripts": {
    "test": "tape test/*.js",
    "docs": "node bin/cli.js -t jsdoc2md/README.hbs -p list lib/jsdoc-to-markdown.js node_modules/jsdoc-parse/lib/jsdoc-parse.js > README.md; echo ",
    "lint": "jshint bin/*.js lib/*.js test/*.js; echo",
    "eg": "node example/render.js; echo"
  },
  "bin": {
    "jsdoc2md": "bin/cli.js"
  },
  "keywords": [
    "handlebars",
    "jsdoc",
    "markdown",
    "generator"
  ],
  "dependencies": {
    "command-line-args": "~0.5.0",
    "console-dope": "~0.3.3",
    "dmd": "~0.3",
    "jsdoc-parse": "~0.3",
    "typical": "^1.0.0"
  },
  "devDependencies": {
    "tape": "^4"
  }
}
