{
  "name": "jsdoc-to-markdown",
  "author": "Lloyd Brookes",
  "version": "0.6.4",
  "description": "Markdown API documentation generator",
  "repository": "https://github.com/75lb/jsdoc-to-markdown",
  "main": "lib/jsdoc-to-markdown.js",
  "files": [
    "bin",
    "lib",
    "LICENSE"
  ],
  "scripts": {
    "test": "tape test/*.js",
    "render": "node test/fixture/render.js; echo",
    "docs": "node bin/cli.js -t jsdoc2md/README.hbs -p list lib/jsdoc-to-markdown.js > README.md; echo ",
    "watch": "baldrick --do 'npm run docs' --when test/input/*/*.js partials/* --change --speak",
    "lint": "jshint bin/*.js lib/*.js test/*.js; echo",
    "index": "jsdoc2md test/fixture/input/**/*.js -v > test/fixture/output/all-with-index.md; echo",
    "all": "npm run render && npm run docs && npm run index; echo",
    "eg": "node example/render-all.js; echo"
  },
  "bin": {
    "jsdoc2md": "bin/cli.js"
  },
  "keywords": [
    "handlebars",
    "jsdoc",
    "markdown",
    "generator"
  ],
  "dependencies": {
    "command-line-args": "~0.5.0",
    "console-dope": "~0.3.3",
    "dmd": "next",
    "jsdoc-parse": "next",
    "typical": "^1.0.0"
  },
  "devDependencies": {
    "tape": "^3"
  }
}
