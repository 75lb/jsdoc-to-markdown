{
  "name": "jsdoc-to-markdown",
  "author": "Lloyd Brookes",
  "version": "0.2.16",
  "description": "Markdown API documentation generator, good for Github projects",
  "repository": "https://github.com/75lb/jsdoc-to-markdown",
  "main": "lib/jsdoc-to-markdown.js",
  "scripts": {
    "test": "",
    "docs": "node test/render.js && node bin/cli.js --skip-heading -t jsdoc2md/README.hbs lib/jsdoc-to-markdown.js > README.md; ",
    "watch": "baldrick --do 'npm run docs' --when test/input/*/*.js partials/* --change --speak",
    "lint": "jshint helpers/*.js bin/*.js lib/*.js; echo",
    "index": "jsdoc2md test/input/**/*.js -p index > test/output/all-with-index.md"
  },
  "bin": {
    "jsdoc2md": "bin/cli.js"
  },
  "keywords": [
    "handlebars",
    "jsdoc",
    "markdown",
    "generator"
  ],
  "dependencies": {
    "array-tools": "^1.1.0",
    "boil-js": "^0.1.0",
    "command-line-args": "^0.4.0",
    "console-dope": "^0.3.3",
    "file-set": "^0.2",
    "jsdoc": "jsdoc3/jsdoc",
    "more-fs": "^0.4.0",
    "object-tools": "^1.0.0",
    "string-tools": "^0.1.4"
  }
}
