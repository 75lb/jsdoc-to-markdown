{
  "name": "jsdoc-to-markdown",
  "author": "Lloyd Brookes",
  "version": "2.0.0",
  "description": "jsdoc-annotated source in, markdown API docs out.",
  "repository": "https://github.com/jsdoc2md/jsdoc-to-markdown",
  "main": "lib/jsdoc-to-markdown.js",
  "license": "MIT",
  "files": [
    "bin",
    "lib",
    "LICENSE"
  ],
  "scripts": {
    "test": "tape test/*.js",
    "docs": "node bin/cli.js lib/jsdoc-to-markdown.js > docs/API.md; echo ",
    "cover": "istanbul cover ./node_modules/.bin/tape test/*.js && cat coverage/lcov.info | ./node_modules/.bin/coveralls && rm -rf coverage; echo"
  },
  "bin": {
    "jsdoc2md": "bin/cli.js"
  },
  "keywords": [
    "jsdoc",
    "markdown",
    "api",
    "generator",
    "javascript",
    "js",
    "documentation"
  ],
  "dependencies": {
    "array-back": "^1.0.3",
    "command-line-tool": "~0.5.2",
    "config-master": "^2.0.3",
    "dmd": "^1.4.2",
    "jsdoc-api": "jsdoc2md/jsdoc-api",
    "jsdoc-parse": "jsdoc2md/jsdoc-parse#next",
    "object-tools": "^2.0.6",
    "walk-back": "^2.0.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.12",
    "tape": "^4.6.0"
  },
  "standard": {
    "ignore": [
      "test",
      "example"
    ]
  }
}
